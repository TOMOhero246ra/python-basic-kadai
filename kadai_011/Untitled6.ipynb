      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "\n",
        "#forループで水～冥の9行を表示\n",
        "\n",
        "for arrays in array:\n",
        "  print(arrays)\n",
        "\n",
        "\n",
        "#Whileループで水～冥の9行を表示\n",
        "#カウンタ変数を用意\n",
        "index = 0\n",
        "while index < len(array): # 'While' を 'while' に修正し、ループ条件を配列の長さに合わせて修正\n",
        "  print(array[index])\n",
        "\n",
        "  #　カウンタ変数の値を1増やす\n",
        "  index = index + 1"
